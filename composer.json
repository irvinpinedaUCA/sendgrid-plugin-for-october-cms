{
  "name": "sarahman/sendgrid-plugin-for-october-cms",
  "description": "SendGrid plugin of sending email in OctoberCMS",
  "type": "library",
  "homepage": "https://github.com/sarahman/sendgrid-plugin-for-october-cms",
  "keywords": ["october", "cms", "admin", "sendgrid", "email"],
  "version": "1.0.0",
  "license": "MIT",
  "authors": [
    {
      "name": "Syed Abidur Rahman",
      "email": "aabid048@gmail.com"
    }
  ],
  "config": {
    "preferred-install": "dist"
  },
  "require": {
    "php": ">=5.5.9",
    "sendgrid/sendgrid": "^7.0"
  },
  "autoload": {
    "psr-4": {
      "Sarahman\\Mailer\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Sarahman\\Mailer\\Tests\\": "tests/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "support": {
    "source": "https://github.com/sarahman/sendgrid-plugin-for-october-cms",
    "issues": "https://github.com/sarahman/sendgrid-plugin-for-october-cms/issues"
  }
}
